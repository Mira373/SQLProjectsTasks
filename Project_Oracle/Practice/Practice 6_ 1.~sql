SELECT  l.location_id, c.country_name,l.street_address, l.city, l.state_province 
from countries c
join LOCATIONS l
on c.country_id=l.country_id;
--- igivea orive magram pirveli sjobs 

SELECT  l.location_id, c.country_name,l.street_address, l.city, l.state_province 
from countries c
 natural join LOCATIONS l









